{
    "name": "khlystou/moonshine-input-datalist",
    "description": "Datalist input extension for MoonShine 2.0",
    "keywords": ["moonshine", "input extension", "datalist"],
    "type": "library",
    "homepage": "https://github.com/khlystou",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/khlystou/moonshine-input-datalist/issues",
        "source": "https://github.com/khlystou/moonshine-input-datalist"
    },
    "authors": [
        {
            "name": "Vasili Khlystou",
            "email": "khlystou@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Khlystou\\DatalistInputExtension\\": "src/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khlystou\\DatalistInputExtension\\Providers\\DatalistServiceProvider"
            ]
        }
    }
}
